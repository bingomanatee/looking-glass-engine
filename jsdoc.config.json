{
  "opts": {
    "plugins": [
      "plugins/markdown",
      "plugins/jsdoc-to-markdown"
    ],
    "recurseDepth": 10,
    "source": {
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "destination": "./docs/",
    "sourceType": "module",
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": [
        "jsdoc",
        "closure"
      ]
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    }
  }
}
